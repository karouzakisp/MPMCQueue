    "version": "0.2.0",
    "configurations": [
        {
            "name": "mpmcqueue_bench",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mpmcqueue_bench",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make mpmcqueue_bench"
        },
